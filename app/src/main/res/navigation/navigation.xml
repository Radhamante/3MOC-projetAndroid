<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/tab_classement">
    <fragment
        android:id="@+id/tab_classement"
        android:name="com.example.projet.ui.mainBottomNavigation.BottomNavigationClassementFragment"
        android:label="SongClassementFragment" >
        <action
            android:id="@+id/action_tab_classement_to_artisteDetailFragment"
            app:destination="@id/artisteDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/artisteDetailFragment"
        android:name="com.example.projet.ui.artisteDetail.ArtisteDetail"
        android:label="ArtisteDetailFragment" />
    <fragment
        android:id="@+id/tab_favoris"
        android:name="com.example.projet.ui.mainBottomNavigation.BottomNavigationFavorisFragement"
        android:label="Favoris" />
    <fragment
        android:id="@+id/tab_recherche"
        android:name="com.example.projet.ui.mainBottomNavigation.BottomNavigationRechercheFragment"
        android:label="Rechercher" />
</navigation>